<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <!--  PRINCIPAL  -->
    <Color x:Key="PrimaryCL">#FF512BD4</Color>
    <Color x:Key="SecondaryCL">#FF2B0B98</Color>
    <Color x:Key="AccentCL">#FF2D6FCC</Color>
    <!--  SEMANTIC  -->
    <Color x:Key="ErrorCL">#FFFF0000</Color>
    <Color x:Key="SuccessCL">#FF00FF00</Color>
    <Color x:Key="WarningCL">#FFFFFF00</Color>
    <!--  NEUTRAL  -->
    <Color x:Key="ForegroundCL">#FF23135E</Color>
    <Color x:Key="BackgroundCL">#FFF7F5FF</Color>
    <Color x:Key="ComplementaryFirstCl">#FF938EA8</Color>
    <Color x:Key="ComplementarySecondCl">#FF8C769A</Color>
    <Color x:Key="ComplementaryThirdCl">#FF7678A0</Color>
</ResourceDictionary>
